<% if @coupon.is_cashback_coupon? %>
  $('#cashback').html("<%= @discount %>")
<% else %>
  $('#discount').html("<%= @discount %>")
<% end %>
CouponUtil.removeDiscountSection();
$('#grand_total').html("<%= @grand_total %>");
$("#coupon_section").html("<%= escape_javascript(render(partial: "carts/coupon_section", locals: {coupon: nil})) %>")